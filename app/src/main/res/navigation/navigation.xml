<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/welcomeFragment">

    <fragment
        android:id="@+id/healthFragment"
        android:name="com.example.moodtrackerproject.ui.HealthFragment"
        android:label="fragment_health"
        tools:layout="@layout/fragment_health" />
    <fragment
        android:id="@+id/notesFragment"
        android:name="com.example.moodtrackerproject.ui.notes.NotesFragment"
        android:label="fragment_notes"
        tools:layout="@layout/fragment_notes" >
        <action
            android:id="@+id/action_notesFragment_to_createNewNoteFragment2"
            app:destination="@id/createNewNoteFragment2" />
    </fragment>
    <fragment
        android:id="@+id/moodFragment"
        android:name="com.example.moodtrackerproject.ui.MoodFragment"
        android:label="fragment_mood"
        tools:layout="@layout/fragment_mood" >
        <action
            android:id="@+id/action_moodFragment_to_addMoodFragment"
            app:destination="@id/addMoodFragment" />
    </fragment>
    <fragment
        android:id="@+id/statisticsFragment"
        android:name="com.example.moodtrackerproject.ui.StatisticsFragment"
        android:label="fragment_statistics"
        tools:layout="@layout/fragment_statistics" />
    <fragment
        android:id="@+id/welcomeFragment"
        android:name="com.example.moodtrackerproject.ui.WelcomeFragment"
        android:label="fragment_welcome_screen"
        tools:layout="@layout/fragment_welcome_screen" >
        <action
            android:id="@+id/action_welcomeFragment_to_loginFragment2"
            app:destination="@id/loginFragment" />
    </fragment>
    <fragment
        android:id="@+id/addMoodFragment"
        android:name="com.example.moodtrackerproject.ui.AddMoodFragment"
        android:label="fragment_add_mood"
        tools:layout="@layout/fragment_add_mood" >
        <action
            android:id="@+id/action_addMoodFragment_to_moodFragment"
            app:destination="@id/moodFragment" />
    </fragment>
    <fragment
        android:id="@+id/loginFragment"
        android:name="com.example.moodtrackerproject.ui.login.LoginFragment"
        android:label="fragment_login_screen"
        tools:layout="@layout/fragment_login_screen" >
        <action
            android:id="@+id/action_loginFragment2_to_notesFragment"
            app:destination="@id/notesFragment" />
    </fragment>
    <fragment
        android:id="@+id/createNewNoteFragment2"
        android:name="com.example.moodtrackerproject.ui.CreateNewNoteFragment"
        android:label="fragment_create_new_note_fragment"
        tools:layout="@layout/fragment_create_new_note" >
        <action
            android:id="@+id/action_createNewNoteFragment2_to_notesFragment"
            app:destination="@id/notesFragment" />
    </fragment>
    <fragment
        android:id="@+id/registrationFragment"
        android:name="com.example.moodtrackerproject.ui.registration.RegistrationFragment"
        android:label="fragment_registration"
        tools:layout="@layout/fragment_registration" />
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.moodtrackerproject.ui.home.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" />
</navigation>